{
    "builds": [
      {
        "src": "florenciaypunto/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "3.10.6"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "florenciaypunto/wsgi.py"
      }
    ]
  }
  